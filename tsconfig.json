{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "paths": {
      "@docs/*": ["docs/*"],
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@theme/*": ["src/components/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
